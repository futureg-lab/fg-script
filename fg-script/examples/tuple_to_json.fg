/**
 * Transform a fg-script to json
 * What does this test ?
 * * Recursion
 * * for loop iteration
 * * Basic concatenation
 */

fn to_json(tup value) -> str {
	if repr_of value is "tup" {
		for (k, v) in value {
			println("{0} : {1}", k, v);
		}
		ret "";
	}
	ret "";
}


