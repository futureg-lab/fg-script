
/**
 * example external function
 * doesn't need to be correct
 * just for testing our lexer
 */

// example external function
extern kron(num) -> bool;

// now let's expose our api
expose fn piececIsBlack(num x, num y) -> bool {
	bool res = null
	if kron (x, y) {
		res = true;
	}
	res = false;
	ret res;
}