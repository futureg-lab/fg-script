/**
 * fg-script lang overview
 * Some comments
 */
num a = 0;
num b = 1;
num N = 10;
num iter = 0;

while iter < N {
    num c = a + b;
    a = b; b = c;
    iter = iter + 1;

    str descr = "";
    // if stmt overview
    if a % 2 == 0 {
        descr = "even";
    } else {
        descr = "odd";
    }

    println("fib {0} = {1}, mod = {2} ({3})", iter, a, a % 2, descr);
    // __mem_debug__();
}