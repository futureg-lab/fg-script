fn pi(num n) -> void {
    num d = n * 3;
    tup arr = [];
    for i in 1 .. d {
        tpush(arr, 2);
    }
    for i in 1 .. d {
        num c = 0;
        num k = d - 1;
        while k >= 0 {
            num v = 10 * arr[k] + c;
            num r = v % (2*k + 1);
            num q = floor(v / (2*k + 1));
            if k > 0 {
                arr[k] = r;
                c = q * k;
            } else {
                arr[k] = q % 10;
                print(floor(q / 10));
                c = 0;
            }
            k = k - 1;
        }
    }
}

pi(100);